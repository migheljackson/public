<li data-equalizer-watch>
  <div class="item-block clearfix">
    <img src="[[+logo_url]]" alt="[[+name]]">
    <div class="row">
      <div class="small-12 large-12 columns prog-title-hdr"><h5><a href="/[[+program_type]]-detail?id=[[+id]]" title="[[+name]] - [[+org_name]]">[[+name]]</a></h5></div>
      <div class="small-12 large-12 columns"><h5 class="price">[[+price]]</h5></div>
    </div>
    <div class="large-12 columns">
      <p class="item-desc">[[+description]]&nbsp;<a href="/[[+program_type]]-detail?id=[[+id]]&ref=search-more"><br><span>read more</span></a></p>
    </div>
    <hr>  
    <div class="row details">
      <!--
      <div class="large-3 columns">
        <div class="circle-text yellow"><p>G</p></div>
      </div>
      -->
      <div class="small-12 large-12 columns type">
        <p><a href="/[[+program_type]]-detail?id=[[+id]]">[[+org_name]]</a></p>
        <p>[[+categories_links]]</p>
      </div>
    </div>
  </div>
</li>

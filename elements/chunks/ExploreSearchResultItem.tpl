<li data-equalizer-watch>
  <div class="item-block clearfix">
    <a href="/[[+program_type]]-detail?id=[[+id]]" title="[[+name]] - [[+org_name]]"><img src="[[+logo_url]]" alt="[[+name]]" class="item-img"></a>
    <div class="row">
      <div class="small-12 large-12 columns prog-title-hdr"><h5><a href="/[[+program_type]]-detail?id=[[+id]]" title="[[+name]] - [[+org_name]]">[[+name]]</a></h5></div>
      <div class="small-12 large-12 columns"><h5 class="price">[[+price]] in person</h5></div>
    </div>
    <div class="large-12 columns">
      <p class="item-desc">[[+description]]</p>
    </div>
    <div class="row">
      <div class="large-12 columns">
        <a href="/[[+program_type]]-detail?id=[[+id]]" title="[[+name]] - [[+org_name]]" class="row button small radius blue">LEARN MORE</a>
      </div>      
    </div>
    <hr>  
    <div class="row details">
      <div class="small-12 large-12 columns type">
        <p>[[+org_name]]</p>
        <p>[[+categories_links]]</p>
      </div>
    </div>
  </div>
</li>

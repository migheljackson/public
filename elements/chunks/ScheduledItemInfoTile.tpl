<!--@description source for scheduled program info tiles
-->
<li>
  <h4 class="event-block-title center">[[+program_type]] info</h4>
  <div class="event-block clearfix">
    <div class="row">
      <div class="small-6 large-12 columns">
        <p style="color: #35BDD3;">[[+price]]</p>
        <p>Registration Deadline: [[+registration_deadline]]</p>
        <p>Recommended Ages: [[+min_age]]&mdash;[[+max_age]]</p>
        <ul class="work-cat">
          [[+categoryHtml]]
        </ul>
      </div>
    <hr/>
    </div>
    <div class="row">
    <div class="small-6 large-12 columns">
      <h5>Organization</h5>
      <p>
        [[+org_logo_html]]
        <a href=[[+org_url]] target="_blank">[[+org_name]]</a>
      </p>
    </div>
  </div>
</li>
